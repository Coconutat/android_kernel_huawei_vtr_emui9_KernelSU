		#size-cells = <2>;
		reg = <0x0 0xe8100000 0x0 0x1000>;
	};

	cci_pmu@e8110000 {
		compatible = "arm,cci-550-pmu,r0";
		reg = <0x0 0xe8110000 0x0 0x80000>;
